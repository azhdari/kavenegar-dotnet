<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>2.0.0</Version>
    <Authors>Mohammad Azhdari</Authors>
    <Product>Kavenegar.NetCore</Product>
    <Description>Kavenegar API Client For .NetStandard 2.0 and later</Description>
    <PackageTags>kavenegar, sms, call, verification</PackageTags>
    <RepositoryUrl>https://github.com/azhdari/kavenegar-dotnet</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageProjectUrl>https://github.com/azhdari/kavenegar-dotnet</PackageProjectUrl>
    <PackageId>kavenegar-dotnetcore</PackageId>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
